    "import hashlib\n",
    "\n",
    "def password(doorId):\n",
    "    i = 0\n",
    "    password = \"\"\n",
    "    while len(password)<8:\n",
    "        hash = hashlib.md5(\"{}{}\".format(doorId,i)).hexdigest()\n",
    "        if hash.startswith(\"0\"*5):\n",
    "            password = password + hash[5]\n",
    "        i+=1\n",
    "    return password\n",
    "\n",
    "password(\"abc\")"
   ]
  }
